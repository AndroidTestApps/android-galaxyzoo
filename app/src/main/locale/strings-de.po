msgstr "Benutzername"

#. The description of the password field when logging in..
msgctxt "prompt_password"
msgid "Password"
msgstr "Passwort"

#. The title of the login button.
msgctxt "action_sign_in"
msgid "Sign in"
msgstr "Anmelden"

#. The title of the login button that will appear on the on-screen keyboard.
msgctxt "action_sign_in_short"
msgid "Sign in"
msgstr "Anmelden"

#. An error message.
msgctxt "error_invalid_password"
msgid "This password is too short."
msgstr "Dieses Passwort ist zu kurz."

#. An error message.
msgctxt "error_incorrect_password"
msgid "This password is incorrect."
msgstr "Dieses Passwort ist nicht korrekt."

#. An error message.
msgctxt "error_field_required"
msgid "This field is required."
msgstr "Dieses Feld ist erforderlich."

#. An error message. We show this when we cannot continue because there is no
#. network connection.
msgctxt "error_no_network"
msgid "No network connection."
msgstr "Keine Netzwerk Verbindung."

#. An error message. We show this when we cannot continue because there is no
#. wi-fi network connection.
msgctxt "error_no_wifi_network"
msgid "No Wi-Fi network connection."
msgstr "Keine WLAN Verbindung."

#. TODO: Make this clearer for people who don't know what an (Internet) server
#. is?
#. An error message. We show this when we cannot continue because something is
#. wrong with the network connection.
msgctxt "error_could_not_connect"
msgid "Cannot connect to the server."
msgstr "Kann keine Verbindung zum Server herstellen."

#. An error message. We show this when we cannot continue because something is
#. wrong with the network connection.
msgctxt "error_no_subjects"
msgid "Cannot download new subjects to classify."
msgstr "Kann keine neue Objekte zum Klassifizieren herunterladen."

#. We offer this button when showing the alert dialog about a network
#. connection problem.
msgctxt "error_button_retry"
msgid "Retry"
msgstr "Wiederholen"

#. We offer this button when showing the alert dialog about a network
#. connection problem.
msgctxt "error_button_cancel"
msgid "Cancel"
msgstr "Abbrechen"

#. An informative message. This lets the user know that they are already
#. logged in to the system.
msgctxt "message_logged_in"
msgid "Logged In."
msgstr "Eingeloggt."

#. The title of the Settings screen to show or change the app preferences.
msgctxt "title_activity_settings"
msgid "Settings"
msgstr "Einstellungen"

#. The title of the preference that controls how many items are downloaded
#. before they are needed.
msgctxt "pref_title_cache_size"
msgid "Download in advance"
msgstr "Im Voraus herunterladen"

#. The title of the preference that controls how many old items should stay on
#. the device.
msgctxt "pref_title_keep_count"
msgid "Keep"
msgstr "Behalte"

msgctxt "pref_title_wifi_only"
msgid "Download via Wi-Fi only"
msgstr "Nur über WLAN herunterladen"

msgctxt "pref_description_wifi_only"
msgid "To reduce carrier data charges, don't download over mobile networks."
msgstr ""
"Downloads über Mobilfunknetze vermeiden, um Kosten für die "
"Datenübertragung zu senken."

